<printer id="printer/Brother-PT-P700">
  <make>Brother</make>
  <model>PT-P700</model>
  <pcmodel>BR9600</pcmodel>
  <mechanism>
    <sublimation/>
    <!--not "color"-->
    <resolution>
      <dpi>
        <x>180</x>
        <y>180</y>
      </dpi>
    </resolution>
    <consumables>
      <comments><en>Brother TZe series tape cartridges</en></comments>
    </consumables>
  </mechanism>
  <url>https://www.brother-usa.com/products/PTP700</url>
  <lang>
    <proprietary />
  </lang>
  <autodetect>
    <general>
      <ieee1284>MFG:Brother;CMD:PT-CBP;MDL:PT-P700;CLS:PRINTER;</ieee1284>
      <commandset>PT-CBP</commandset>
      <description>Brother PT-P700</description>
      <manufacturer>Brother</manufacturer>
      <model>PT-P700</model>
    </general>
  </autodetect>
  <functionality>B</functionality>
  <driver>ptouch</driver>
  <!--no "contrib_url"-->
  <!--no "comments"-->
</printer>
